let a = 1;
let b = 2;

if (a == b) {
    print "TRUE";
} else {
    print "FALSE"; // false
}


a = 1;
b = 1;

if (a == b) {
    print "TRUE"; // true
} else {
    print "FALSE";
}


// more conditionals with logical operations.

if (true and true) {
    print "true and true is infact true"; 
} 

if (false and true) {
    print "this is never going to get printed";
}

if (true or true) {
    print "true or true is true.";
}

if (true or false) {
    print "true and false is still true";
}